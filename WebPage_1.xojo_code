#tag WindowCode
	#tag Event
		Sub Opening()
		  
		  If accordion_on_open then
		    accordion_redraw
		  end if
		  
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Sub accordion_redraw()
		  Dim Height_of_tallest_control As Integer 
		  Dim Row_is_totally_invisible As Boolean
		  
		  Dim i,j, top_reset As Integer
		  
		  top_reset= accordion_top_row
		  
		  If Not(accordion_refence_initalised) Then
		    
		    Redim accordion_controls(-1)
		    Redim accordion_control_tops(-1)
		    
		    ///live Stuff
		    For i  = 0 To Self.LastControlIndex
		      Dim c As WebUIControl = WebUIControl(Self.ControlAt(i))
		      
		      If c IsA WebTimer Then
		        //Do nothing
		      Else
		        accordion_controls.Add c
		        accordion_control_tops.Add c.Top
		      End If 
		      
		    Next
		    
		    For  i = 0 To accordion_control_tops.LastIndex       
		      
		      If accordion_disctinct_top_values.IndexOf (accordion_control_tops(i)) =-1 Then
		        accordion_disctinct_top_values.Add accordion_control_tops(i)
		      End If
		      
		    Next
		    
		    accordion_disctinct_top_values.Sort
		    
		    accordion_refence_initalised=True
		  End If
		  
		  
		  For i = 0 To accordion_disctinct_top_values.LastIndex
		    
		    Height_of_tallest_control=0  
		    Row_is_totally_invisible=True
		    
		    For  j = 0 To accordion_control_tops.LastIndex        
		      
		      If accordion_control_tops(j)= accordion_disctinct_top_values(i) Then 
		        
		        accordion_controls(j).top=accordion_top_row
		        
		        //if the control is a WebContainer we need to UpdateBrowser
		        //FC 66559 - WebContainer position not updated in WebPage opening event
		        if accordion_controls(j) isa WebContainer then
		          accordion_controls(j).UpdateBrowser
		        End If
		        
		        // checkbox moves right/left bug
		        // https://tracker.xojo.com/xojoinc/xojo/-/issues/68949
		        if accordion_controls(j) isa WebCheckbox then
		          accordion_controls(j).Style.ForegroundColor = accordion_controls(j).Style.ForegroundColor
		        End If
		        
		        If Height_of_tallest_control < accordion_controls(j).height Then 
		          Height_of_tallest_control=accordion_controls(j).Height 
		        End If 
		        
		        If accordion_controls(j).Visible Then
		          Row_is_totally_invisible=False
		        End If
		        
		      End If 
		      
		    Next
		    
		    If Row_is_totally_invisible Then
		      
		    Else
		      accordion_top_row=accordion_top_row+Height_of_tallest_control+accordion_spacing
		    End If
		    
		  Next
		  
		  accordion_top_row=top_reset
		  
		  
		End Sub
	#tag EndMethod


