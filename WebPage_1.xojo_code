#tag Events Button1
	#tag Event
		Sub Pressed()
		  accordion_redraw
		  
		  Hint1.Visible=false
		  Hint2.Visible=true
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Checkbox1
	#tag Event
		Sub ValueChanged()
		  If me.Value then
		    TextArea1.Visible=True
		  else
		    TextArea1.Visible=False
		  end if
		  
		  Hint2.Visible=false
		  Hint3.Visible=true
		  Hint3.left= 35
		  
		  accordion_redraw
		End Sub
	#tag EndEvent
